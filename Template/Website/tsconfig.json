{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "olive-page": [ "wwwroot/lib/olive.mvc/dist/olivepage" ],
            "olive-config": [ "wwwroot/lib/olive.mvc/dist/config" ]
        },
        "module": "amd",
        "target": "es5",
        "sourceMap": true,
        "noImplicitUseStrict": true,
        "noImplicitAny": false,
        "noEmitOnError": true,
        "removeComments": false
    }, "compileOnSave": true,
    "exclude": [
        "wwwroot/lib",
        "typings",
        "lib"
    ]
}
